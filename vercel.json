{
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "api/pyservice.py", "use": "@vercel/python" },
    { "src": "api/index.ts", "use": "@vercel/node" }
  ],
   "rewrites": [
   
    {
      "source": "/internal/aiservice/(.*)",
      "destination":"/api/pyservice.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.ts"
    }
  ]
    
}